<ul class="sidebar-profile">
  <li class="sidebar-item">
    <%= link_to (dashboard_path) do %>
      <i class="fa fa-dashboard" aria-hidden="true"></i>
      <span class="profile-text">Dashboard</span>
    <% end %>
  </li>
  <li class="sidebar-item">
    <%= link_to (edit_user_registration_path) do %>
      <i class="fa fa-database" aria-hidden="true"></i>
      <span class="profile-text">Update Profile</span>
    <% end %>
  </li>
  <li class="sidebar-item">
    <%= link_to (profile_picture_user_path(@user)) do %>
      <i class="fa fa-picture-o" aria-hidden="true"></i>
      <span class="profile-text">Profile Pictures</span>
    <% end %>
  </li>
  <li class="sidebar-item">
    <%= link_to (phone_verification_user_path(@user)) do %>
      <i class="fa fa-phone-square" aria-hidden="true"></i>
      <span class="profile-text">Phone Verification</span>
    <% end %>
  </li>
  <li class="sidebar-item">
    <%= link_to (company_logo_user_path(@user)) do %>
      <i class="fa fa-picture-o" aria-hidden="true"></i>
      <span class="profile-text">Company Logo</span>
    <% end %>
  </li>

  <li class="sidebar-item">
    <%= link_to (current_user) do %>
      <i class="fa fa-user-circle-o" aria-hidden="true"></i>
      <span class="profile-text">View Profile</span>
    <% end %>
  </li>
